<div class="container">
  <div class="row">
    <h3>Users</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= link_to user.name %>
              <%= 1/0 %>
              <br>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
